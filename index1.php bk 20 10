         $("#NoteEdit").html(ajaxData);
         $("#NoteEdit").modal('show',{backdrop: 'true'});
       }
     });
      });
$(document).on('click', '.edit_comment', function (e) {
  var m = $(this).attr("id");
    $.ajax({
       url: "pages/comment_edit.php",
       type: "GET",
       data : {id: m,},
       success: function (ajaxData){
         $("#CommentEdit").html(ajaxData);
         $("#CommentEdit").modal('show',{backdrop: 'true'});
       }
     });
      });	
