		"language": {
			          "search": "Filter records:"
			        }
		
	}); 
    $("#example2").DataTable({
      "paging": true,
      "lengthChange": false,
      "searching": false,
      "ordering": false,
      "info": true,
      "autoWidth": false,	
    });
	$("#example3").DataTable({
		"scrollX"  : true,
	    "scrollY"  : '300px',
		"ordering": true,
		"paging": false,
		"info": true,
		
	});
	$("#example4").DataTable({
		"scrollX"  : true,
