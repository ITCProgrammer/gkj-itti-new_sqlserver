<script>
$(document).on('click', '.edit_status', function (e) {
  var m = $(this).attr("id");
    $.ajax({
       url: "pages/status_edit.php",
       type: "GET",
       data : {id: m,},
       success: function (ajaxData){
         $("#StatusEdit").html(ajaxData);
         $("#StatusEdit").modal('show',{backdrop: 'true'});
       }
     });
      });
$(document).on('click', '.edit_status_in', function (e) {
  var m = $(this).attr("id");
